{
  "name": "@ulid/shared",
  "version": "0.1.0",
  "description": "Internal utilities used in @ulid/ulid",
  "module": "esm/index.js",
  "main": "dist/index.js",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "npm run build:esm && npm run build:commonjs",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:commonjs": "tsc --module commonjs --outDir dist"
  },
  "devDependencies": {
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "author": "Dinislam Tebuev",
  "license": "MIT"
}
