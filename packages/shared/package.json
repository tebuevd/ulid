{
  "name": "@ulid/shared",
  "version": "0.1.1-alpha.1.0",
  "description": "Internal utilities used in @ulid/ulid",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./dist/index.js"
  },
  "types": "./dts/src/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:commonjs",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --outdir=esm --minify --sourcemap",
    "build:commonjs": "esbuild src/index.ts --bundle --platform=node --outdir=dist --minify --sourcemap"
  },
  "devDependencies": {
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "author": "Dinislam Tebuev",
  "license": "MIT"
}
